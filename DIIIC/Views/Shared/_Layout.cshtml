                            <h3 style="margin: 10px 0 0 10px">18:00:00</h3>                        
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/Template/dist/img/IST_Logo.png" class="img-circle" alt="User Image" />
                    </div>
                    <div class="pull-left info">
                        <p>UserAdmin</p>
                        <a><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>

                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header">MAIN NAVIGATION</li>
                    <li class="active treeview">
                        <a href="@Url.Action("Index","Home")">
                            <i class="fas fa-tachometer-alt"></i> <span style="padding-left:10px">Dashboard</span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="far fa-file-alt"></i> <span style="padding-left:10px">Registo de Stock</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li class="active"><a href="@Url.Action("RenovationOrders","Home")"><i class="fa fa-circle-o"></i>Pedidos de renovação</a></li>
                            <li><a href="@Url.Action("HistorialOrders","Home")"><i class="fa fa-circle-o"></i>Pedidos efetuados</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fas fa-capsules"></i> <span style="padding-left:5px">Medicamentos Registados</span>
                        </a>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <div class="content-wrapper">
            @RenderBody()
            <!-- /End.Main content -->
        </div>
    </div><!-- ./wrapper -->

    @RenderSection("Scripts", required: false)

</body>
</html>
